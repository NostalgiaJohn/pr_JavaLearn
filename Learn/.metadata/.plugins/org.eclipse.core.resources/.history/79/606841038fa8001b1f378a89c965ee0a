package com.fs;

public class Demo1 {
	/*
	 * 自动补齐:Alt + / 
	 */
	/**
	 * main方法
	 * 
	 * @param args main方法所需参数
	 */
	public static void main(String[] args) {
		System.out.println("你好");
		
		// 单行注释:Ctrl + /
		// Ctrl + Alt + 方向键上下 复制
		
		/*
		 * Ctrl + shift + / 多行注释 System.out.println(); System.out.println();
		 * System.out.println();
		 */
	}
	
}
